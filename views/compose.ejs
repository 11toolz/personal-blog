<%- include('partials/header'); %>
<h1>Compose</h1>
<form action="/compose" method="post" class="form-horizontal">
  <div class="form-group">

      <label for="entryTitle">Title</label>
      <input type="text" name="entryTitle" class="form-control">

    </div>
    <div class="form-group">

        <label for="entryBody">Post</label>
        <textarea  class="form-control" name="entryBody" rows="5" cols="30"></textarea>

  </div>

  <button type="submit" name="button" class="btn btn-primary">Publish</button>
</form>

<%- include('partials/footer'); %>
